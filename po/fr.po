#: gedlib/messages.c:38 gedlib/messages.c:45 src/gedlib/messages.c:45
msgid "You must identify a database."
msgstr "Vous devez sélectionner une base"

#: gedlib/messages.c:39
msgid "What directory holds the LifeLines database?"
msgstr "Quel répertoire contient la base LifeLines ?"

#: gedlib/messages.c:40 gedlib/messages.c:47 src/gedlib/messages.c:47
msgid "enter path: "
msgstr "entrer le chemin :"

#: gedlib/messages.c:41 gedlib/messages.c:48 src/gedlib/messages.c:48
msgid "There is no LifeLines database in that directory."
msgstr "Il n'y a pas de base LifeLines dans ce répertoire."

